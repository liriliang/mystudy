<?xml version="1.0" encoding="utf-8"?>

<web-app version="4.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd">
	<display-name>Archetype Created Web Application</display-name>
	<!-- 获取列表 -->
	<servlet>
		<servlet-name>WorkList</servlet-name>
		<display-name>WorkList</display-name>
		<description></description>
		<servlet-class>com.jd.workNote.WorkList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WorkList</servlet-name>
		<url-pattern>/WorkList</url-pattern>
	</servlet-mapping>

	<!-- 登录 -->
	<servlet>
		<servlet-name>login</servlet-name>
		<display-name>login</display-name>
		<description></description>
		<servlet-class>com.jd.workNote.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
</web-app>
